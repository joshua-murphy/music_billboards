<h1 class="title">Edit Artist</h1>
<% if params[:billboard_id] %>
  <% @billboard = Billboard.find(params[:billboard_id]) %>
  <%= render "form" %>
<% else %>
  <ul class="nav_bar">
    <li><%= link_to "Home", billboards_path, class: "nav_bar_button" %></li>
    <li><%= link_to "Cancel", artists_path(@artist), class: "nav_bar_button" %></li>
  </ul>

  <%= render "form" %>
<% end %>